   "source": [
    "import time\n",
    "\n",
    "alarm_time = input('Enter the alarm time (HH:MM): ')\n",
    "while True:\n",
    "    current_time = time.strftime(\"%H:%M\")\n",
    "    if current_time == alarm_time:\n",
    "        print(\"Time to wake up!\")\n",
    "        break\n",
    "    time.sleep(1)\n"
   ]
  }
